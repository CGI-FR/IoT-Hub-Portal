<MudDialog>
    <DialogContent>
        <LoRaDeviceTelemetry DeviceId="@DeviceId" />
    </DialogContent>
    <DialogActions>
        <MudButton id="cancel-telemetry-dialog" OnClick="Cancel">Cancel</MudButton>
    </DialogActions>
</MudDialog>

@code {
    [CascadingParameter] MudDialogInstance MudDialog { get; set; } = default!;

    [Parameter]
    public string DeviceId { get; set; } = default!;

    void Cancel() => MudDialog.Cancel();
}
